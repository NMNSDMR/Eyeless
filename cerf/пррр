def apply_algorithm(s):
    count = 0
    while "1" in s or "100" in s:
        if "100" in s:
            s = s.replace("100", "0001", 1)
            count += 3
        else:
            s = s.replace("1", "00", 1)
            count += 1
    return count

original_string = "1" + "0" * 33
zero_count = apply_algorithm(original_string)
print(zero_count)