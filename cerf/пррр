def apply_program(s):
    while "25" in s or "355" in s or "4555" in s:
        if "25" in s:
            s = s.replace("25", "4", 1)
        elif "355" in s:
            s = s.replace("355", "2", 1)
        else:
            s = s.replace("4555", "3", 1)
    return s

original_string = "2" + "5" * 81
result = apply_program(original_string)
print(result)