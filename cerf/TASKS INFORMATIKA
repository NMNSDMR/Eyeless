for i in range(1000000):
    s = bin(i)[2::]
    s += str(s.count(’1’) % 2)
    s += str(s.count(’1’) % 2)
    if int(s, 2) > 100:
        print(int(s, 2))
        break
2.
d = []
for n in range(1000):
   n2 = bin(n)[2:]
   n2 += n2[-3:] if n % 3 == 0 else bin((n % 3) * 3)[2:]
   r = int(n2, 2)
   if r > 151:
      d. append (r)
print (min(d))
